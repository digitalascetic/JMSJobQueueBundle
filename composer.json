{
  "name": "digitalascetic/job-queue-bundle",
  "version": "5.0.0",
  "description": "Allows to run and schedule Symfony console commands as background jobs.",
  "keywords": [
    "job",
    "queue",
    "background",
    "cron",
    "scheduled"
  ],
  "homepage": "http://jmsyst.com/bundles/JMSJobQueueBundle",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Johannes M. Schmitt",
      "email": "schmittjoh@gmail.com"
    },
    {
      "name": "Eduard Deza",
      "email": "ed@ascetic.io"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/framework-bundle": "^5.2",
    "symfony/process": "^5.2",
    "symfony/twig-bundle": "^5.2",
    "symfony/orm-pack": "^2.1"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "*",
    "symfony/yaml": "*",
    "doctrine/orm": "*",
    "symfony/test-pack": "^1.0",
    "symfony/monolog-bundle": "^3.6"
  },
  "suggest": {
    "symfony/twig-bundle": "Required when using the webinterface."
  },
  "autoload": {
    "psr-4": {
      "JMS\\JobQueueBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JMS\\JobQueueBundle\\Tests\\": "Tests/"
    }
  },
  "target-dir": "JMS/JobQueueBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
